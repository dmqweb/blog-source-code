{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "editPost": "hexo clean && hexo generate && hexo deploy",
    "addPost": "hexo clean && hexo g && hexo deploy && set HEXO_ALGOLIA_INDEXING_KEY=bfa5090f87840ffa546f9e1cb4bc1ef0 && hexo algolia",
    "algolia": "set HEXO_ALGOLIA_INDEXING_KEY=bfa5090f87840ffa546f9e1cb4bc1ef0 && hexo algolia",
    "serve": "hexo clean && hexo g && hexo s",
    "preserve":"echo 服务开始启动",
    "postserve":"echo 启动成功",
    "new": "hexo new --categories vue --tags vue,面试题   标题",
    "push": "git add . && git commit -m 'pushRemote' && git push gitee",
    "postpush":"echo 推送成功",
    "pull": "git pull gitee master"
  },
  "hexo": {
    "version": "6.3.0"
  },
  "dependencies": {
